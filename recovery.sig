[TEMPLATES]
TEMPLATE1 = PRIMITIVE_MYSQL1
TEMPLATE2 = PRIMITIVE_MYSQL2
TEMPLATE3 = PRIMITIVE_MYSQL3
TEMPLATE4 = PRIMITIVE_PHP

[PRIMITIVE_MYSQL1]
DESCRIPTION = MySQL Signature
EXTENSION = sql
BEGIN=MYSQL_BEGIN 
FOOTER=MYSQL_FOOTER 

[MYSQL_BEGIN]
\x2D\x2D\x20\x4D\x79\x53\x51\x4C\x20\x64 = 0 | 512

[MYSQL_FOOTER]
\x2D\x2D\x20\x44\x75\x6D\x70\x20\x63 = 0 | 512

[PRIMITIVE_MYSQL2]
DESCRIPTION = MySQL Signature 2
EXTENSION = sql2
BEGIN=MYSQL2_BEGIN 
FOOTER=MYSQL2_FOOTER 

[MYSQL2_BEGIN]
-- MySQL dump = 0 | 1024

[MYSQL2_FOOTER]
-- Dump completed on = 0 | 1024


[PRIMITIVE_MYSQL3]
DESCRIPTION = MySQL Signature 3
EXTENSION = sql3
BEGIN=MYSQL3_BEGIN 
FOOTER=MYSQL3_FOOTER 

[MYSQL3_BEGIN]
MySQL = 0 | 1024

[MYSQL3_FOOTER]
Dump = 0 | 1024



[PRIMITIVE_PHP]
DESCRIPTION = PHP Signature
EXTENSION = php
BEGIN=PHP_BEGIN 
FOOTER=PHP_FOOTER 

[PHP_BEGIN]
\x3C\x3F\x70\x68\x70 = 0 | 512

[PHP_FOOTER]
\x3F\x3E = 0 | 512
